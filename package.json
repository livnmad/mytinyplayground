{
  "name": "survey-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/server/index.js",
    "build": "tsc && webpack --mode production",
    "dev": "webpack serve --mode development", 
  "dev:up": "docker-compose -f docker-compose.dev.yml up -d --build",
  "dev:down": "docker-compose -f docker-compose.dev.yml down",
  "prod:up": "docker-compose -f docker-compose.prd.yml up -d --build",
  "prod:down": "docker-compose -f docker-compose.prd.yml down"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  }
}
